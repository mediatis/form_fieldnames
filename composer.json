{
  "name": "mediatis/form_fieldnames",
  "type": "typo3-cms-extension",
  "description": "Mediatis Form Field Names",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3",
    "extension",
    "form",
    "field",
    "name"
  ],
  "homepage": "https://www.mediatis.de",
  "authors": [
    {
      "email": "info@mediatis.de",
      "homepage": "https://www.mediatis.de",
      "name": "Mediatis AG",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^12.4 || ^13.4",
    "typo3/cms-form": "^12.4 || ^13.4"
  },
  "autoload": {
    "psr-4": {
      "Mediatis\\FormFieldnames\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "form_fieldnames"
    }
  },
  "replace": {
    "typo3-ter/form_fieldnames": "self.version"
  }
}
